{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "keys": "ctrl+c"
    },
    {
      "command": "paste",
      "keys": "ctrl+v"
    },
    {
      "command": "find"
    },
    {
      "command": "unbound",
      "keys": "ctrl+shift+f"
    },
    {
      "command": "unbound",
      "keys": "alt+left"
    },
    {
      "command": "unbound",
      "keys": "alt+right"
    },
    {
      "command": "unbound",
      "keys": "alt+up"
    },
    {
      "command": "unbound",
      "keys": "alt+down"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "keys": "alt+shift+d"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "left"
      },
      "keys": "alt+h"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "down"
      },
      "keys": "alt+j"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "right"
      },
      "keys": "alt+l"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "up"
      },
      "keys": "alt+k"
    }
  ],
  "copyFormatting": "none",
  "copyOnSelect": false,
